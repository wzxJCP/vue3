#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/2021/ZObject/SpringBoot3_Vue3/vue3/node_modules/.pnpm/vite@6.3.5_sass@1.89.0/node_modules/vite/bin/node_modules:/mnt/d/2021/ZObject/SpringBoot3_Vue3/vue3/node_modules/.pnpm/vite@6.3.5_sass@1.89.0/node_modules/vite/node_modules:/mnt/d/2021/ZObject/SpringBoot3_Vue3/vue3/node_modules/.pnpm/vite@6.3.5_sass@1.89.0/node_modules:/mnt/d/2021/ZObject/SpringBoot3_Vue3/vue3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/2021/ZObject/SpringBoot3_Vue3/vue3/node_modules/.pnpm/vite@6.3.5_sass@1.89.0/node_modules/vite/bin/node_modules:/mnt/d/2021/ZObject/SpringBoot3_Vue3/vue3/node_modules/.pnpm/vite@6.3.5_sass@1.89.0/node_modules/vite/node_modules:/mnt/d/2021/ZObject/SpringBoot3_Vue3/vue3/node_modules/.pnpm/vite@6.3.5_sass@1.89.0/node_modules:/mnt/d/2021/ZObject/SpringBoot3_Vue3/vue3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
